{
  "presets": ["react", "es2015", "stage-2"],
  "plugins": [
    "babel-plugin-transform-class-properties",
    "transform-react-remove-prop-types",
    ["babel-plugin-styled-components", {
      "ssr": true
    }]
  ],
  "env": {
    "test": {
      "plugins": [
        "babel-plugin-transform-es2015-modules-commonjs",
      ]
    }
  }
}
